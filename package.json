    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "typescript": "^4.8.4",
    "vitepress": "^1.0.0-alpha.30",
    "vitest": "^0.29.7",
    "vue": "^3.2.45"
  },
  "dependencies": {
    "@riadh-adrani/array-utils": "^1.0.3",
